      formatYumCount:function (n) {
      if (!n) {
        return '0';
      }
      if (n < 1000) {
        return '' + n;
      }
      if (n < 9.9995e5) {
        return Math.round(n / 1e3) + 'k';
      }
      if (n < 9.9995e8) {
        return Math.round(n / 1e6) + 'm';
      }
      if (n < 9.9995e11) {
        return Math.round(n / 1e9) + 'b';
      }
      return Math.round(n / 1e12) + 't';
    },
